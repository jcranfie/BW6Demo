enable&quot;/>&#xa;                &lt;/tns4:ExceptionAuditLocalEnabled>&#xa;            &lt;/tns4:AuditOptions>&#xa;            &lt;tns15:RuntimeContext>&#xa;                &lt;tns15:ServiceName>&#xa;                    &lt;xsl:value-of select=&quot;bw:getModuleProperty(&amp;quot;BW.MODULE.NAME&amp;quot;)&quot;/>&#xa;                &lt;/tns15:ServiceName>&#xa;                &lt;tns15:Application>&#xa;                    &lt;xsl:value-of select=&quot;bw:getModuleProperty(&amp;quot;BW.APPLICATION.NAME&amp;quot;)&quot;/>&#xa;                &lt;/tns15:Application>&#xa;                &lt;tns15:ApplicationVersion>&#xa;                    &lt;xsl:value-of select=&quot;bw:getModuleProperty(&amp;quot;BW.APPLICATION&amp;quot;)&quot;/>&#xa;                &lt;/tns15:ApplicationVersion>&#xa;                &lt;tns15:AppNode>&#xa;                    &lt;xsl:value-of select=&quot;bw:getModuleProperty(&amp;quot;BW.APPNODE.NAME&amp;quot;)&quot;/>&#xa;                &lt;/tns15:AppNode>&#xa;                &lt;tns15:Hostname>&#xa;                    &lt;xsl:value-of select=&quot;bw:getHostName()&quot;/>&#xa;                &lt;/tns15:Hostname>&#xa;            &lt;/tns15:RuntimeContext>&#xa;            &lt;tns7:AuditEvent>&#xa;                &lt;tns2:CorrelationId>&#xa;                    &lt;xsl:value-of select=&quot;$BWProcessHeaders/tns2:CorrelationId&quot;/>&#xa;                &lt;/tns2:CorrelationId>&#xa;                &lt;tns7:EventId>&#xa;                    &lt;xsl:value-of select=&quot;tib:uuid()&quot;/>&#xa;                &lt;/tns7:EventId>&#xa;                &lt;tns7:AuditTimeStamp>&#xa;                    &lt;xsl:value-of select=&quot;current-dateTime()&quot;/>&#xa;                &lt;/tns7:AuditTimeStamp>&#xa;                &lt;tns7:EventType>&#xa;                    &lt;xsl:value-of select=&quot;&amp;quot;Response&amp;quot;&quot;/>&#xa;                &lt;/tns7:EventType>&#xa;                &lt;tns7:Message>&#xa;                    &lt;xsl:value-of select=&quot;concat(&amp;quot;Response from &amp;quot;, $varBackEnd)&quot;/>&#xa;                &lt;/tns7:Message>&#xa;                &lt;xsl:if test=&quot;$log.audit.payload.enable = true()&quot;>&#xa;                    &lt;tns7:Payload>&#xa;                        &lt;xsl:copy-of select=&quot;$BackEndResponse&quot;/>&#xa;                    &lt;/tns7:Payload>&#xa;                &lt;/xsl:if>&#xa;                &lt;tns7:EventTimeStamp>&#xa;                    &lt;xsl:value-of select=&quot;$EndTime&quot;/>&#xa;                &lt;/tns7:EventTimeStamp>&#xa;                &lt;tns7:PreviousEventTimeStamp>&#xa;                    &lt;xsl:value-of select=&quot;$StartTime&quot;/>&#xa;                &lt;/tns7:PreviousEventTimeStamp>&#xa;            &lt;/tns7:AuditEvent>&#xa;            &lt;xsl:copy-of select=&quot;$StartAuditEvent&quot;/>&#xa;        &lt;/tns8:SendAuditEventRequest>&#xa;    &lt;/xsl:template>&#xa;&lt;/xsl:stylesheet>" expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xslt1.0"/>
                </tibex:inputBindings>
                <bpws:targets>
                    <bpws:target linkName="AssignBackEndResponseToInvoke"/>
                </bpws:targets>
            </bpws:invoke>
        </bpws:flow>
    </bpws:scope>
</bpws:process>
